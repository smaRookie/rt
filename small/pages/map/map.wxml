<!--pages/map/map.wxml-->
<view>
  <!-- <map></map> -->
  <!-- <view class="">
  
  </view> -->
  <map id="map" 
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    scale="16"
    controls="{{controls}}"
    bindcontroltap="controltap"
    markers="{{markers}}"
    bindmarkertap="markertap"
    polyline="{{polyline}}"
    bindregionchange="regionchange"
    show-location
    show-compass
    enable-overlooking
    enable-zoom = true
    enable-rotate
    enable-satellite: true
    enable-traffic
    controls="{{controls}}"
    class="mapcon">
    <cover-view>
  
    </cover-view>
  </map>
</view>
